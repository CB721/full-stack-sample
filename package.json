{
  "name": "full-stack-sample",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "build:prod": "webpack server.js dist/bundle.js -p",
    "seed": "cd seeds && node index.js"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CB721/full-stack-sample.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^6.3.0",
    "nodemon": "^1.18.7",
    "webpack-cli": "^3.3.12"
  },
  "bugs": {
    "url": "https://github.com/CB721/full-stack-sample/issues"
  },
  "homepage": "https://github.com/CB721/full-stack-sample#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "connect-session-sequelize": "^6.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mysql2": "^2.1.0",
    "node-sass": "^4.14.0",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.7",
    "validator": "^13.0.0"
  }
}
